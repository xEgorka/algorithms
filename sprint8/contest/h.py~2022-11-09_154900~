from typing import List


def search(p: str, text: str) -> List[int]:
    result = []
    s = p + '#' + text
    pi = [0] + [None] * (len(p) - 1)
    pi_prev = 0
    for i in range(1, len(s)):
        k = pi_prev
        while k and s[k] != s[i]:
            k = pi[k - 1]
        if s[k] == s[i]:
            k += 1
        if i < len(p):
            pi[i] = k
        pi_prev = k
        if k == len(p):
            result.append(i - 2 * len(p))
    return result


def main() -> int:
    text, s, t = input(), input(), input()
    result = search(s, text)
    print(*result)
    return 0


if __name__ == '__main__':
    main()
