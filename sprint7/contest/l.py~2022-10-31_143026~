def main() -> int:
    n, M = list(map(int, input().split(' ')))
    weight = list(map(int, list(input().split(' '))))
    dp = [[0 for x in range(M + 1)] for x in range(2)]
    for i in range(n + 1):
        k = i % 2
        for j in range(M + 1):
            if weight[i - 1] <= j:
                dp[k][j] = max(
                    dp[(k - 1)][j],
                    weight[i - 1] + dp[(k - 1)][j - weight[i - 1]],
                )
            else:
                dp[k][j] = dp[(k - 1)][j]
    print(dp[n % 2][M])
    return 0

if __name__ == '__main__':
    main()
