def main() -> int:
    n, M = list(map(int, input().split(' ')))
    weight = list(map(int, list(input().split(' '))))
    dp = [[0 for x in range(M + 1)] for x in range(2)]
    for i in range(n + 1):
        for j in range(M + 1):
            if weight[i - 1] <= j:
                dp[i % 2][j] = max(
                    dp[(i - 1) % 2][j],
                    weight[i - 1] + dp[(i - 1) % 2][j - weight[i - 1]],
                )
            else:
                dp[i % 2][j] = dp[(i - 1) % 2][j]
    print(dp[n % 2][M])
    return 0


if __name__ == '__main__':
    main()
