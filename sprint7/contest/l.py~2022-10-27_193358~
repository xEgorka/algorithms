def main() -> int:
    n, M = list(map(int, input().split(' ')))
    weight = list(map(int, list(input().split(' '))))
    dp = [[0 for x in range(M + 1)] for x in range(n + 1)]
    for i in range(n + 1):
        for j in range(M + 1):
            if not i or not j:
                dp[i][j] = 0
            elif weight[i - 1] <= j:
                dp[i][j] = max(
                    dp[i - 1][j], weight[i - 1] + dp[i - 1][j - weight[i - 1]]
                )
            else:
                dp[i][j] = dp[i - 1][j]
    print(dp[n][M])
    return 0


if __name__ == '__main__':
    main()
